package com.quiz.question.model;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import lombok.Data;

@Entity
@Data
public class Question {
	
	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Integer id;
	private String catagory;
	private String questionTitle;
	private String option1;
	private String option2;
	private String option3;
	private String option4;
	private String rightAnswere;
	private String difficultyLevel;
	
	
	  @Override public String toString() { return "Question [id=" + id +
	  ", questionTitle=" + questionTitle + ", catagory=" + catagory + ", option1="
	  + option1 + ", option2=" + option2 + ", option3=" + option3 + ", option4=" +
	  option4 + ", rightAnswere=" + rightAnswere + ", difficultyLevel=" +
	  difficultyLevel + "]"; }
	 
	
	
	
}
